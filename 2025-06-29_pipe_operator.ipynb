   "source": [
    "from typing import Any\n",
    "\n",
    "class Pipe:\n",
    "    def __init__(self, value):\n",
    "        self.value = value\n",
    "    \n",
    "    def __or__(self, func: Any) -> Any:\n",
    "        return func(self.value)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "5da200f4",
   "metadata": {},
   "outputs": [],
   "source": [
    "def add(a, b) -> Pipe:\n",
    "    return Pipe(a + b)"
   ]
  },
